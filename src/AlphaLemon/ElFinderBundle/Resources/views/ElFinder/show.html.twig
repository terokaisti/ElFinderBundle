			var diff = self.diff(odata.files.concat(pdata && pdata.tree ? pdata.tree : []));

			diff.removed.length && self.remove(diff);
			diff.added.length   && self.add(diff);
			diff.changed.length && self.change(diff);
			return dfrd.resolve(diff);
		});
		
		return dfrd;
	}
	
